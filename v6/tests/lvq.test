#!/usr/bin/env tclkit
# %renumber<^\s*test >%

source [file join [file dir [info script]] initests.tcl]

cd lvq

test 0 {lua test} {
    exec make install
    set out [exec tests/test.lua]\n
    regsub -all {\m(function|table|row|view): 0x\w+} $out {\1: 0x[...]} out
    exec diff -u - ../tests/lvq.ref << $out
} {}

cd ..

::tcltest::cleanupTests
