#!/usr/bin/env tclkit
# debug build tests
# %renumber<^\s*test >%

source [file join [file dir [info script]] initests.tcl]

test 0 {load package} {
    package require vq
} $version

catch { vq debug version; testConstraint debug 1 }

test 1 {} debug { vq debug version } $version

test 2 {check availability} debug {
    list [catch { vq debug ? } e] $e
} {1 {bad option "?": must be version}}

::tcltest::cleanupTests
