#!/usr/bin/env tclkit
# view get tests

source [file join [file dir [info script]] initests.tcl]

# %renumber<^\s*test >%

test 0 {load extension} {
  package require vlerq
} 3

test 1 {view exists} -body {
  view
} -returnCodes error -result {wrong # args: should be "view v ... | ..."}

test 2 {}  { view 5 get #     } 5 
test 3 {}  { view 5 get *     } {{} {} {} {} {}} 
test 4 {}  { view 5 get * *   } {} 
